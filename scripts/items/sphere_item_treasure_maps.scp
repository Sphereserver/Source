[TYPEDEF T_L1MAP_CHEST]
//LEVEL1MAP Treasure map Chest Spawn

ON=@DCLICK
	IF <DISTANCE>>3
		RETURN 1
	endif
	IF ( <LINK.TOPOBJ.UID> != <SRC.UID> )
		RETURN 1
	endif
	IF ((<SRC.FINDLAYER(layer_hand1).BASEID>==i_PICKAXE) && (<SRC.MINING>>50.0))
		SRC.ANIM=0B
		SOUND=0126
		SRC.NEWITEM=TM_LOOT_1 //Treasure map random poor loot
		SRC.NEW.P=<P>
		SRC.MESSAGE=WOW a treasure !! OOPS!!
		LINK.REMOVE         //remove linked deciphered map
		SRC.NEWITEM=i_Treas_Spawn_VEasy //Treasure map very easy monster spawn
		SRC.NEW.P=<P>
		SRC.NEWITEM=i_Treas_Spawn_VEasy //Treasure map very easy monster spawn
		SRC.NEW.P=<P>
		SRC.NEWITEM=i_Treas_Spawn_VEasy //Treasure map very easy monster spawn
		SRC.NEW.P=<P>
		REMOVE            //remove trigger plant
		RETURN 1
	endif
	RETURN 1

