VERSION=0.56b

[DEFNAME TREASURE_MAPS]
TMOS       0FFFFFED4 //Treasure Map Offset
TMS        08c //Treasure Map Size   200x200 pixels - pins are in pixels
TMTILES    { i_plant_fern 1 i_plant_rushes 1 i_plant_fern 1 i_plant_fern_5 1 i_plant_bulrushes 1 i_plant_grasses 1 i_plant_fern_3 1 i_plant_fern_4 1 }

[TYPEDEF T_L1MAP]
//LEVEL1MAP Deciphering

ON=@DCLICK
	IF ! <SRC.RESTEST 1 i_pickaxe>
		SRC.MESSAGE=Where is your Pickaxe?
		RETURN 1
	endif
	IF (<SRC.CARTOGRAPHY><50.0)
		SRC.MESSAGE=You are not a good enough cartographer for this!
		RETURN 1
	endif
	// MOREP = the treasure location = MOREX,MOREY
	IF (<MORE><0FFFF)
		NAME=Deciphered Treasure Map
		TYPE=T_MAP
		MORE1=((<MOREX> -180)*010000)+(<MOREY> -180)
		MORE2=((<MOREX> +180)*010000)+(<MOREY> +180)
		PIN=105,105   // dead center.
		SRC.HOME=<SRC.P>
		SRC.P=<MOREP>
		SRC.NEWITEM=<DEF.TMTILES> //Place trigger random tile on UO Map
		SRC.NEW.TYPE=T_L1MAP_CHEST
		SRC.NEW.TIMER=-1
		SRC.NEW.ATTR=attr_static|attr_move_never
		SRC.NEW.LINK=<UID>
		SRC.NEW.P=<SRC.P>
		SRC.P=<SRC.HOME>   // put us back where we really are.
		RETURN 0
	endif

